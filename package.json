{
  "name": "template-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run generate-docs",
    "preview": "npm run build && next start",
    "start": "next start",
    "lint": "next lint",
    "generate-docs": "npx typedoc --exclude '**/*.tsx' --entryPointStrategy expand ./src --out ./public/docs",
    "typecheck": "tsc --noEmit",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx,mdx,js,jsx}\" --cache",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,mdx,js,jsx}\" --cache",
    "clear:cache": "rm -r ./.next ./node_modules",
    "update:latest": "npx npm-check-updates -u"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/lib/db/seed/seed.ts"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^13.5.4",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.2",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "eslint": "8.43.0",
    "eslint-config-next": "^14.2.13",
    "lucide-react": "^0.256.0",
    "next": "^14.2.13",
    "next-themes": "^0.2.1",
    "postcss": "8.4.24",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.46.2",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "uuid": "^9.0.0",
    "zod": "^3.22.2",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@types/node": "^20.6.3",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  }
}
